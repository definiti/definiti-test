package x

verification IsNonEmpty {
  "The list should not be empty"
  [A](list: List[A]) => {
    list.nonEmpty()
  }
}

context tests {{{
  test verification IsNonEmpty {
    accept List[String](1)
    accept List[Number]("")
    accept List[String]("", 1, "")
    refuse List[Number](1, "", "", 2)
  }
}}}