package x

type Person {
  names: List[String]
}

context tests {{{
  test verification IsNonEmpty {
    accept Person {
      names: List[String](1)
    }
    accept Person {
      names: List[Number]("")
    }
    accept Person {
      names: List[String]("", 1, "")
    }
    refuse Person {
      names: List[Number](1, "", "", 2)
    }
  }
}}}