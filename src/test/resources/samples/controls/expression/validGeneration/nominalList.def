package x

verification IsNonEmpty {
  "The list should not be empty"
  [A](list: List[A]) => {
    list.nonEmpty()
  }
}

context tests {{{
  test verification IsNonEmpty {
    accept List[String]("")
    accept List[String]("", "")
    accept List[String]("", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "", "")
    refuse List[String]()
  }
}}}